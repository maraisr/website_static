{
	"extends": "@marais/tsconfig",
	"compilerOptions": {
		"target": "esnext",
		"lib": [
			"DOM",
			"ESNext"
		],
		"strict": true,
		"isolatedModules": true,
		"sourceMap": false,
		"noImplicitAny": false,
		"jsx": "preserve"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
